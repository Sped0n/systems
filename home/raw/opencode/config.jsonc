{
  "$schema": "https://opencode.ai/config.json",
  // general
  "theme": "system",
  "autoupdate": false,
  "model": "openrouter/moonshotai/kimi-k2.5",
  "default_agent": "ask",
  // providers
  "provider": {
    "openrouter": {
      "models": {
        "moonshotai/kimi-k2.5": {
          "options": {
            "provider": {
              "sort": "throughput",
            },
            "reasoning": {
              "enabled": true,
            },
          },
        },
        "x-ai/grok-4.1-fast": {
          "options": {
            "reasoning": {
              "enabled": true,
            },
          },
        },
        "minimax/minimax-m2.5": {
          "options": {
            "provider": {
              "sort": "throughput",
            },
            "reasoning": {
              "enabled": true,
            },
          },
        },
      },
    },
    "yescode": {
      "npm": "@ai-sdk/openai",
      "name": "YesCode",
      "options": {
        "baseURL": "https://co-cdn.yes.vg/v1",
        "apiKey": "{env:YESCODE_API_KEY}",
      },
      "models": {
        "gpt-5.2": {
          "name": "GPT-5.2",
          "cost": {
            "input": 1.05,
            "output": 8.4,
            "cache_read": 0.105,
          },
          "limit": {
            "context": 400000,
            "output": 128000,
          },
          "options": {
            "reasoningEffort": "high",
            "textVerbosity": "low",
            "reasoningSummary": "auto",
            "include": ["reasoning.encrypted_content"],
          },
        },
        "gpt-5.3-codex": {
          "name": "GPT-5.3-Codex",
          "cost": {
            "input": 0.75,
            "output": 6,
            "cache_read": 0.075,
          },
          "limit": {
            "context": 272000,
            "output": 128000,
          },

          "options": {
            "reasoningEffort": "high",
            "textVerbosity": "low",
            "reasoningSummary": "auto",
            "include": ["reasoning.encrypted_content"],
          },
        },
      },
    },
  },
  // agents
  "agent": {
    "plan": {
      "model": "yescode/gpt-5.2",
      "temperature": 0.1,
      "tools": {
        "grep": false,
        "glob": false,
        "list": false,
        "lsp": false,
        "websearch": false,
        "webfetch": false,
      },
      "permission": {
        "skill": {
          "*": "deny",
          "ast-grep": "allow",
        },
      },
    },
    "build": {
      "model": "yescode/gpt-5.3-codex",
      "temperature": 0.3,
      "tools": {
        "grep": false,
        "glob": false,
        "list": false,
        "lsp": false,
        "websearch": false,
        "webfetch": false,
      },
      "permission": {
        "bash": {
          "git *": "allow",
          "git commit *": "ask",
          "git push *": "deny",
        },
      },
    },
    "general": {
      "model": "openrouter/moonshotai/kimi-k2.5",
      "temperature": 0.25,
      "tools": {
        "grep": false,
        "glob": false,
        "list": false,
        "lsp": false,
        "websearch": false,
        "webfetch": false,
      },
      "permission": {
        "bash": {
          "git *": "allow",
          "git commit *": "ask",
          "git push *": "deny",
        },
      },
    },
    "explore": {
      "model": "openrouter/x-ai/grok-4.1-fast",
      "temperature": 0.0,
      "tools": {
        "grep": false,
        "glob": false,
        "list": false,
        "lsp": false,
        "websearch": false,
        "webfetch": false,
      },
      "permission": {
        "skill": {
          "*": "deny",
          "ast-grep": "allow",
        },
      },
    },
    "ask": {
      "model": "yescode/gpt-5.2",
    },
    "commit-message-writer": {
      "model": "openrouter/minimax/minimax-m2.5",
    },
    "50": {
      "model": "openrouter/minimax/minimax-m2.5",
    },
    "99": {
      "model": "openrouter/moonshotai/kimi-k2.5",
    },
  },
  // mcp servers
  "mcp": {
    "jina-mcp-server": {
      "type": "remote",
      "url": "https://mcp.jina.ai/v1?include_tools=search_web,read_url,parallel_search_web,parallel_read_url",
      "headers": {
        "Authorization": "{env:JINA_API_KEY}",
      },
    },
  },
  // keybinds
  "keybinds": {
    "input_submit": "shift+return,ctrl+return,alt+return",
    "input_newline": "return",
  },
}
